{
  "editor.formatOnSave": true,
  "editor.deafultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": [
    "source.addMissingImports",
    "source.fixAll.eslint"
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "astro",
    "typescript",
    "typescriptreact"
  ],
  "[svelte]": {
    "editor.formatOnSave": true,
    "editor.deafultFormatter": "svelte.svelte-vscode"
  }

}