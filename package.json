{
  "name": "astro-site",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "prebuild": "run-s prebuild:*",
    "prebuild:feed": "ts-node --esm script/getFeed.ts",
    "prebuild:github": "ts-node --esm script/getGitHub.ts",
    "dev": "astro dev",
    "start": "astro preview",
    "preview": "astro preview",
    "build": "astro build",
    "astro": "astro",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint src/**/*.{ts,astro,svelte} --ignore-path .gitignore",
    "lint:prettier": "prettier --check src/**/*.{ts,astro,svelte} --ignore-path .gitignore",
    "fix": "run-s fix:*",
    "fix:eslint": "eslint --fix src/**/*.{ts,astro,svelte} --ignore-path .gitignore",
    "fix:prettier": "prettier --write src/**/*.{ts,astro,svelte} --ignore-path .gitignore",
    "prepare": "husky install"
  },
  "dependencies": {
    "@astrojs/svelte": "^4.0.4",
    "@octokit/graphql": "^7.0.2",
    "algoliasearch": "^4.20.0",
    "astro": "^3.6.3",
    "autoprefixer": "^10.4.16",
    "clsx": "^2.0.0",
    "cssnano": "^6.0.1",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "remark-comment": "^1.0.0",
    "remark-gfm": "^4.0.0",
    "rss-parser": "^3.13.0",
    "svelte": "^4.2.7",
    "unified": "^11.0.4",
    "unist-util-is": "^6.0.0",
    "unist-util-visit": "^5.0.0",
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "@astrojs/partytown": "^2.0.2",
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@octokit/graphql-schema": "^14.44.1",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-astro": "^0.29.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsx-a11y": "^6.8.0",
    "eslint-plugin-svelte": "^2.35.1",
    "fs-extra": "^11.2.0",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.1.0",
    "prettier-plugin-astro": "^0.12.2",
    "prettier-plugin-svelte": "^3.1.2",
    "sass": "^1.69.5",
    "svelte-eslint-parser": "^0.33.1",
    "ts-node": "^10.9.1"
  }
}
