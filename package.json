{
  "name": "astro-site",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev --project tsconfig.json",
    "start": "astro dev",
    "prebuild": "run-s prebuild:*",
    "prebuild:feed": "ts-node --esm script/getFeed.ts",
    "prebuild:github": "ts-node --esm script/getGitHub.ts",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "run-p lint:eslint",
    "lint:eslint": "eslint src/**/*.{ts,astro,svelte} --ignore-path .gitignore",
    "lint:prettier": "prettier --check src/**/*.{ts,astro,svelte} --ignore-path .gitignore",
    "fix": "run-s fix:*",
    "fix:eslint": "eslint --fix src/**/*.{ts,astro,svelte} --ignore-path .gitignore",
    "fix:prettier": "prettier --write src/**/*.{ts,astro,svelte} --ignore-path .gitignore",
    "prepare": "husky install"
  },
  "dependencies": {
    "@astrojs/svelte": "^3.0.0",
    "@octokit/graphql": "^5.0.6",
    "algoliasearch": "^4.17.2",
    "astro": "^2.6.4",
    "autoprefixer": "^10.4.14",
    "clsx": "^1.2.1",
    "cssnano": "^6.0.1",
    "date-fns": "^2.30.0",
    "dotenv": "^16.1.4",
    "remark-comment": "^1.0.0",
    "remark-gfm": "^3.0.1",
    "rss-parser": "^3.13.0",
    "svelte": "^3.59.1",
    "unified": "^10.1.2",
    "unist-util-is": "^5.2.1",
    "unist-util-visit": "^4.1.2",
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "@astrojs/partytown": "^1.2.3",
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "@octokit/graphql-schema": "^14.10.1",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.3.1",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-astro": "^0.27.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-svelte3": "^4.0.0",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prettier-plugin-astro": "^0.10.0",
    "prettier-plugin-svelte": "^2.10.1",
    "sass": "^1.63.4",
    "ts-node": "^10.9.1"
  }
}
