{
  "name": "astro-site",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "script": "run-s script:*",
    "script:feed": "node script/getFeed.mjs",
    "script:github": "node -r dotenv/config script/getGitHubUserContent.mjs",
    "script:cv": "node -r dotenv/config script/getGitHubRepositoryContent.mjs",
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "start": "astro preview",
    "astro": "astro",
    "lint": "run-p lint:*",
    "lint:prettier": "biome format ./src",
    "lint:eslint": "biome lint ./src",
    "fix": "run-s fix:*",
    "fix:prettier": "biome format --write ./src",
    "fix:eslint": "biome check --apply ./src",
    "prepare": "husky"
  },
  "dependencies": {
    "@astrojs/svelte": "^5.2.0",
    "@octokit/graphql": "^8.0.1",
    "algoliasearch": "^4.22.1",
    "astro": "^4.5.5",
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.5",
    "remark-comment": "^1.0.0",
    "rss-parser": "^3.13.0",
    "svelte": "^4.2.12",
    "unified": "^11.0.4",
    "unist-util-is": "^6.0.0",
    "unist-util-visit": "^5.0.0",
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "@astrojs/partytown": "^2.0.4",
    "@biomejs/biome": "1.6.1",
    "@commitlint/cli": "^19.2.0",
    "@commitlint/config-conventional": "^19.1.0",
    "@octokit/graphql-schema": "^15.3.0",
    "@types/fs-extra": "^11.0.4",
    "@types/node": "^20.11.28",
    "fs-extra": "^11.2.0",
    "husky": "^9.0.11",
    "npm-run-all2": "^6.1.2"
  }
}
