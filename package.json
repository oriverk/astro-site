{
  "name": "astro-site",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "prebuild": "run-s prebuild:*",
    "prebuild:feed": "ts-node --esm script/getFeed.ts",
    "prebuild:github": "ts-node --esm script/getGitHub.ts",
    "dev": "astro dev",
    "start": "astro preview",
    "preview": "astro preview",
    "build": "astro build",
    "astro": "astro",
    "lint": "run-p lint:*",
    "lint:eslint": "eslint src/**/*.{ts,astro,svelte} --ignore-path .gitignore",
    "lint:prettier": "prettier --check src/**/*.{ts,astro,svelte} --ignore-path .gitignore",
    "fix": "run-s fix:*",
    "fix:eslint": "eslint --fix src/**/*.{ts,astro,svelte} --ignore-path .gitignore",
    "fix:prettier": "prettier --write src/**/*.{ts,astro,svelte} --ignore-path .gitignore",
    "prepare": "husky install"
  },
  "dependencies": {
    "@astrojs/svelte": "^3.1.1",
    "@octokit/graphql": "^7.0.1",
    "algoliasearch": "^4.19.1",
    "astro": "^2.10.12",
    "autoprefixer": "^10.4.15",
    "clsx": "^2.0.0",
    "cssnano": "^6.0.1",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "remark-comment": "^1.0.0",
    "remark-gfm": "^3.0.1",
    "rss-parser": "^3.13.0",
    "svelte": "^4.2.0",
    "unified": "^11.0.2",
    "unist-util-is": "^6.0.0",
    "unist-util-visit": "^5.0.0",
    "url-join": "^5.0.0"
  },
  "devDependencies": {
    "@astrojs/partytown": "^1.2.3",
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@octokit/graphql-schema": "^14.27.2",
    "@types/fs-extra": "^11.0.1",
    "@types/node": "^20.5.1",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-astro": "^0.28.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-svelte": "^2.33.0",
    "fs-extra": "^11.1.1",
    "husky": "^8.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.2",
    "prettier-plugin-astro": "^0.11.1",
    "prettier-plugin-svelte": "^3.0.3",
    "sass": "^1.66.1",
    "svelte-eslint-parser": "^0.33.0",
    "ts-node": "^10.9.1"
  }
}
